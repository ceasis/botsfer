# OpenAI (currently used by native audio -> LLM flow)
app.openai.enabled=true
app.openai.api-key=sk-REPLACE_ME
app.openai.base-url=https://api.openai.com/v1
app.openai.transcription-model=gpt-4o-mini-transcribe
app.openai.text-model=gpt-4o-mini

